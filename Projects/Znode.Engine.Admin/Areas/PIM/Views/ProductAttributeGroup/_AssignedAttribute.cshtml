@using Znode.Engine.Admin.ViewModels;
@model PIMAttributeGroupMapperListViewModel
@{
    string controllerName = ViewContext.RouteData.Values["controller"].ToString();
}
<div id=@Model.GridModel.FrontObjectName tabchild="true">
    @{
        if (!@Model.IsNonEditable)
        {
            <div class="pull-right padding-left action-dropdown">
                @{Html.RenderAction("GetUnAssignedAttribute", controllerName, new { @pimAttributeGroupId = @Model.PimAttributeGroupId });}
            </div>
                    }
                    <div class="col-sm-12 list-container margin-top">
                        <div class="attribut-list">
                            @Html.Partial("~/Views/DynamicGrid/_DynamicGrid.cshtml", Model.GridModel)
                        </div>
                    </div>
    }
</div>
<script>
    $("#DropDownId1").closest(".dropdown").hide()
</script>
<script>
    $(document).ready(function () { EditableGridEvent.Init(); })
    $(".input-text").change(function () {
        validateForm = CommonHelper.prototype.ValidateDisplayOrderField(this);
    });
</script>