@model Znode.Engine.Admin.ViewModels.AttributeGroupListViewModel

<div id="grid-container dirtyignore">
    <div class="col-sm-12 list-container">
        <div class="pull-right margin-bottom">
            @{Html.RenderAction("GetUnAssignedMediaAttributeGroups", "AttributeFamily", new { @id = @Model.AttributeFamilyId, @area = "MediaManager"  });}
        </div>
        @if (Model?.AttributeGroups?.Count > 0)
        {
            @Html.Partial("~/Areas/MediaManager/Views/AttributeFamily/_AssignedGroupCollapsibleGrid.cshtml", Model)
        }
        else
        {
            <div class="col-sm-12 no-record nopadding">
                <p>@Admin_Resources.NoRecordFoundText</p>
            </div>
        }
    </div>
</div>