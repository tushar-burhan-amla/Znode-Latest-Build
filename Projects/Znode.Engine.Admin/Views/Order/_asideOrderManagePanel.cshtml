<div class="manage-order-tabs">
    <ul class="nav nav-tabs" id="OrderAsidePannel">

        <!-- Tabs button Starts -->
        <li id="customer-tab-link" class="active">
            <a data-toggle="tab" href="#customer-tab" data-attr="data-toggle-tab">@Admin_Resources.CustomerHeader</a>
        </li>

        <li id="shipping-cart-tab-link" class="disabled">
            <a data-toggle="tab" href="#shipping-cart-tab" data-attr="data-toggle-tab">@Admin_Resources.LabelCartAndShipping</a>
        </li>

        <li id="review-placeorder-tab-link" class="disabled">
            <a data-toggle="tab" href="#review-placeorder-tab" data-attr="data-toggle-tab">@Admin_Resources.LableReview</a>
        </li>

        <li id="payment-tab-link" class="disabled">
            <a data-toggle="tab" href="#payment-tab" data-attr="data-toggle-tab">@Admin_Resources.LablePaymentAndPlaceOrder</a>
        </li>

        
        <!-- Tabs button Ends -->
        <!-- Next tab option button Starts -->
        @*<a class="btn-text btn-text-secondary pull-right" style="display:none" id="btnPlaceOrder" data-test-selector="btnPlaceOrder" onclick="Order.prototype.SubmitOrder();">
            @Admin_Resources.LabelPlaceYourOrder
        </a>*@
        <button class="btn-text btn-text-secondary pull-right" id="btnNextTab" data-test-selector="btnNextTabSelect" onclick="OrderTabs.prototype.ContinueOrder();">
            <span>Next: </span>
            <span id="btnNextTabName">@Admin_Resources.LabelAddProduct</span>
        </button>

        <!-- Next tab option button Ends -->

        <!-- Place Order button Starts -->
        <a class="btn-text btn-text-secondary pull-right" style="display:none" id="btnPlaceOrder" data-test-selector="btnPlaceOrder" onclick="Order.prototype.SubmitOrder();">
            @Admin_Resources.LabelPlaceYourOrder
        </a>
        <!-- Place Order button Ends -->

    </ul>

</div>





