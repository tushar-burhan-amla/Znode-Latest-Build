            <li>@Html.AuthorizedRawActionLink(@Admin_Resources.HeaderAssociatedAccounts, "GetAssociatedAccountList", "Price", new { @priceListId = Model.PriceListId, @listName = string.Equals(Model.ListName, null) ? Model.ListName : HttpUtility.UrlEncode(Model.ListName)},new { @data_test_selector = "linkAssociatedAccounts" })</li>
