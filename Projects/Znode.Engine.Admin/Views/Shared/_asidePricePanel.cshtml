    <div class="slide-container">
        <ul class="aside-panel">
            <li>@Html.AuthorizedRawActionLink(@Admin_Resources.LabelPriceListInfo, "Edit", "Price", new { @priceListId = Model.PriceListId },new { @data_test_selector = "linkPriceListInfo" })</li>
