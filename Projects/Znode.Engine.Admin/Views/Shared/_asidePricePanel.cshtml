            <li>@Html.AuthorizedRawActionLink(@Admin_Resources.HeaderAssociatedStores, "GetAssociatedStoreList", "Price", new { @priceListId = Model.PriceListId, @listName = string.Equals(Model.ListName, null) ? Model.ListName : HttpUtility.UrlEncode(Model.ListName) },new { @data_test_selector = "linkAssociatedStores" })</li>
