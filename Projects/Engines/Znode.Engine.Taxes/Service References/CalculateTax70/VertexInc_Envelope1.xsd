  <xsd:include schemaLocation="indirect/VertexInc-Reversal.xsd" />
  <xsd:include schemaLocation="indirect/VertexInc-Rollback.xsd" />
  <xsd:include schemaLocation="indirect/VertexInc-Exists.xsd" />
  <xsd:include schemaLocation="taxgis/VertexInc-FindChangedTaxAreaRequest.xsd" />
  <xsd:include schemaLocation="taxgis/VertexInc-FindChangedTaxAreaResponse.xsd" />
  <xsd:include schemaLocation="taxgis/VertexInc-ChangedTaxAreaRequest.xsd" />
  <xsd:include schemaLocation="taxgis/VertexInc-ChangedTaxAreaResponse.xsd" />
  <xsd:include schemaLocation="taxgis/VertexInc-FindTaxAreasRequest.xsd" />
  <xsd:include schemaLocation="taxgis/VertexInc-FindTaxAreasResponse.xsd" />
  <xsd:include schemaLocation="taxgis/VertexInc-TaxAreaRequest.xsd" />
  <xsd:include schemaLocation="taxgis/VertexInc-TaxAreaResponse.xsd" />
