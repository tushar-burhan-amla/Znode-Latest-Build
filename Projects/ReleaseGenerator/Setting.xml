      <Generate name="Content" action="Copy"/>
      <Generate name="TypeScriptCompile" action="Copy"/>
      <Generate name="Compile" action="Copy"/>
    </Project>
    
    <Project name="Znode.Engine.Connector" directory="Libraries"  action="Copy">
      <Generate name="Content" action="Copy"/>
      <Generate name="TypeScriptCompile" action="Copy"/>
      <Generate name="Compile" action="Copy"/>
