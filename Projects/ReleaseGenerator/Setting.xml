<?xml version="1.0" encoding="utf-8" ?>
<Folders>
  <Folder source="..\SharedLibraries"
                 target="..\SharedLibraries" action="Copy"></Folder>

  <Folder source="Libraries\ZNode.Libraries.Report"
              target="..\Projects\Libraries\ZNode.Libraries.Report" action="Copy"></Folder>

  <Solution source="Znode.Multifront.sln"
            target="..\Projects" sharedlibpath="..\SharedLibraries" configEvn="Release">

    <Project name="Znode.Engine.Admin" action="Copy">
      <Generate name="Content" action="Copy"/>
      <Generate name="TypeScriptCompile" action="Copy"/>
      <Generate name="Compile" action="Copy"/>
    </Project>
	
    <Project name="Znode.Engine.Api" action="Copy">
      <Generate name="Content" action="Copy"/>
      <Generate name="TypeScriptCompile" action="Copy"/>
      <Generate name="Compile" action="Copy"/>
